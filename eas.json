{
  "cli": {
    "version": ">= 16.3.3",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "REDACTED_API_URL",
        "EXPO_PUBLIC_SUPABASE_URL": "REDACTED_SUPABASE_URL",
        "EXPO_PUBLIC_SUPABASE_ANON_KEY": "REDACTED_SUPABASE_ANON_KEY"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./REDACTED_SERVICE_ACCOUNT"
      }
    }
  }
}
