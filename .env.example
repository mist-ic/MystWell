# Supabase Credentials
SUPABASE_URL=YOUR_SUPABASE_PROJECT_URL
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY # Keep this secure!
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@[YOUR-HOST]:5432/postgres

# Google Cloud Credentials
# Ensure the path points to your downloaded service account key file
GOOGLE_APPLICATION_CREDENTIALS=./mystwell-gcp-key.json.example # Or the actual path if different
GOOGLE_PROJECT_ID=YOUR_GCP_PROJECT_ID
GOOGLE_SPEECH_RECOGNIZER_NAME=projects/YOUR_GCP_PROJECT_ID/locations/global/recognizers/YOUR_RECOGNIZER_ID_OR_DEFAULT

# Google Gemini API Key (Optional - only if not using ADC via service account)
# GEMINI_API_KEY=YOUR_GEMINI_API_KEY

# Redis Connection (for BullMQ Task Queue)
REDIS_HOST=localhost
REDIS_PORT=6379
# REDIS_PASSWORD=
# REDIS_USER=

# Optional: RLS Workaround Flag (Defaults to true if not set)
# USE_SERVICE_ROLE_FOR_RECORDINGS=true

# Optional: Server Port
# PORT=3000 